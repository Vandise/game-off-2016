{
  "name": "hackablemi",
  "version": "1.0.0",
  "description": "Game",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "test": "NODE_ENV=test mocha --require ignore-styles --opts spec/mocha.opts ${FILE:-spec}",
    "start": "./node_modules/.bin/gulp watch",
    "lint": "eslint --fix ${FILE:-src spec} && echo '\\033[0;32mAll Good\\033[0m' || true",
    "cover": "nyc --require babel-core/register -a npm test"
  },
  "nyc": {
    "exclude": [
      "spec",
      "dist"
    ],
    "include": [
      "src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napcs/game-off-2016.git"
  },
  "author": "Benjamin J. Anderson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vandise/game-off-2016/issues"
  },
  "homepage": "https://github.com/Vandise/game-off-2016#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-webpack-alias": "^2.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "css-loader": "^0.11.0",
    "enzyme": "^2.0.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "expose-loader": "^0.7.1",
    "gulp": "^3.8.11",
    "gulp-shell": "^0.4.1",
    "gulp-util": "^3.0.4",
    "ignore-styles": "^4.0.0",
    "jsdom": "^9.4.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.2.0",
    "nyc": "^6.4.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.2.5",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
